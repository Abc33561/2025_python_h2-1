# 字串 (String)
name = "20"
message = '歡迎使用Python'

# 整數 （Integer）
age = 25
score = 100

# 浮點數 (Float)
height = 175.6
price = 99.99

# 布林值(BooLean)
is_student = True
has_liscense = False

# 檢查變數類型
print(name)
print(type(name))       # <class 'str'>  
print(type(age))        # <class 'int'> 
print(type(height))     # <class 'fLoat'> 
print(type(is_student)) # <class 'bool'> 

